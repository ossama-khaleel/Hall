@model Hall_Boking_System.Models.Visa

@{
   Layout=null;
   var name = ViewBag.CustomersName;
}

<!DOCTYPE html>
<html lang="en">
<head>
<link rel="stylesheet" type="text/css" href="~/Assests/styles/bootstrap-4.1.2/bootstrap.min.css">
<link href="~/Assests/plugins/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" type="text/css" href="~/Assests/plugins/OwlCarousel2-2.3.4/owl.carousel.css">
<link rel="stylesheet" type="text/css" href="~/Assests/plugins/OwlCarousel2-2.3.4/owl.theme.default.css">
<link rel="stylesheet" type="text/css" href="~/Assests/plugins/OwlCarousel2-2.3.4/animate.css">
<link href="~/Assests/plugins/jquery-datepicker/jquery-ui.css" rel="stylesheet" type="text/css">
<link href="~/Assests/plugins/colorbox/colorbox.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" type="text/css" href="~/Assests/styles/main_styles.css">
<link rel="stylesheet" type="text/css" href="~/Assests/styles/responsive.css">
<link rel="stylesheet" type="text/css" href="~/Assests/styles/contact.css">
<link rel="stylesheet" type="text/css" href="~/Assests/styles/contact_responsive.css">
<!--==========================================================================================-->
    <!-- Required meta tags-->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Colrolib Templates">
    <meta name="author" content="Colrolib">
    <meta name="keywords" content="Colrolib Templates">
    <!-- Title Page-->
    <title>Au Form Wizard</title>
    <!-- Icons font CSS-->
    <link href="~/BokingAssests/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <link href="~/BokingAssests/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <!-- Font special for pages-->
    <link href="https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">
    <!-- Vendor CSS-->
    <link href="~/BokingAssests/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/BokingAssests/vendor/datepicker/daterangepicker.css" rel="stylesheet" media="all">
    <!-- Main CSS-->
    <link href="~/BokingAssests/css/main.css" rel="stylesheet" media="all">
</head>
<body>
<div class="super_container">
	<!-- Header -->
	<header class="header">
		<div class="header_content d-flex flex-row align-items-center justify-content-start">
			<div class="logo"><a href="#" style="width:130px;font-size:21px;text-decoration:none">Hall Boking</a></div>
			<div class="ml-auto d-flex flex-row align-items-center justify-content-start">
				<nav class="main_nav">
					<ul class="d-flex flex-row align-items-start justify-content-start">
						<li class="active"><a style="text-decoration:none" asp-controller="Home" asp-action="Index">Home</a></li>
						<li><a style="text-decoration:none" asp-controller="Home" asp-action="AboutUs">About Us</a></li>
						<li><a style="text-decoration:none" href="#contact">Contactus</a></li>
						<li><a style="text-decoration:none" asp-controller="Testimonials" asp-action="Create">Testimonial</a></li>
						   @if (name!=null)
						   {
							  <li><a style="text-decoration:none" asp-controller="LoginAndRegestration" asp-action="Logout">Logout</a></li>
							  <li><a style="text-decoration:none;color:white">Hello,@name</a></li>
							  <li><a style="text-decoration:none" asp-controller="DashBoard" asp-action="Customer">My Profil</a></li>
						   }
						   else
						   {  
							   <li><a style="text-decoration:none" asp-controller="LoginAndRegestration" asp-action="Login">Sign In</a></li>
							   <li><a style="text-decoration:none" asp-controller="LoginAndRegestration" asp-action="Register">Sign Up</a></li>
						   }
					</ul>
				</nav>
				<!-- Hamburger Menu -->
			</div>
		</div>
	</header>
	<!-- Home -->
	<div class="home">
		<div class="home_slider_container">
			<div class="owl-carousel owl-theme home_slider">
				<!-- Slide -->
				<div class="slide">
					<div class="background_image" style="background-image: url()"></div>
					<div class="home_container">
						<div class="container">
							<div class="row">
								<div class="col">
									<div class="home_content text-center">
										<div class="home_title">A Luxury Stay</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
            </div>
         </div>
    </div>

<div class="page-wrapper bg-img-1 p-t-165 p-b-100">
        <div class="wrapper wrapper--w680">
            <div class="card card-1">
                <div class="card-body">
                    <div class="tab-content">
                        <div class="tab-pane active" id="tab3">
                            <form asp-action="Create">
							 <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <div class="input-group">
                                    <label class="label">Visa Number:</label>
                                    <input asp-for="VisaNumber" class="input--style-1" />
                                    <span asp-validation-for="VisaNumber" class="text-danger"></span>
                                </div>
                                <div class="input-group">
                                    <label class="label">Visa Amount:</label>
                                    <input asp-for="VisaAmount" class="input--style-1" />
                                    <span asp-validation-for="VisaAmount" class="text-danger"></span>
                                </div>
								<div hidden="hidden" class="input-group">
                                    <label class="label">Visa Amount:</label>
                                    <input asp-for="VisaName" class="input--style-1" />
                                    <span asp-validation-for="VisaName" class="text-danger"></span>
                                </div>
                                <div class="form-group">
									<input type="submit" value="Create" class="btn" style="background-color: #ffa37b;color: white" />
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
</div>

<!-- Footer -->
	<footer class="footer">
		<div class="footer_content">
			<div class="container">
				<div class="row">
					<div class="col">
						<div class="footer_logo_container text-center">
							<div class="footer_logo">
								<a href="#"></a>
								<div>The River</div>
								<div>since 1945</div>
							</div>
						</div>
					</div>
				</div>
				<div class="row footer_row">
					
					<!-- Address -->
					<div class="col-lg-3">
						<div class="footer_title">Our Address</div>
						<div class="footer_list">
							<ul>
								<li>Beach Str. 345</li>
								<li>67559 Miami</li>
								<li>USA</li>
							</ul>
						</div>
					</div>

					<!-- Reservations -->
					<div class="col-lg-3">
						<div class="footer_title">Reservations</div>
						<div class="footer_list">
							<ul>
								<li>Tel: 345 5667 889</li>
								<li>Fax; 6783 4567 889</li>
								<li>reservations@hotelriver.com</li>
							</ul>
						</div>
					</div>

					<!-- Newsletter -->
					<div class="col-lg-3">
						<div class="footer_title">Newsletter</div>
						<div class="newsletter_container">
							<form action="#" class="newsletter_form" id="newsletter_form">
								<input type="email" class="newsletter_input" placeholder="Your email address" required="required">
								<button class="newsletter_button">Subscribe</button>
							</form>
						</div>
					</div>

					<!-- Footer images -->
					<div class="col-lg-3">
						<div class="certificates d-flex flex-row align-items-start justify-content-lg-between justify-content-start flex-lg-nowrap flex-wrap">
							<div class="cert"><img src="~/Assests/images/cert_1.png" alt=""></div>
							<div class="cert"><img src="~/Assests/images/cert_2.png" alt=""></div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="copyright">
			<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
			Copyright &copy;<script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="fa fa-heart-o" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib</a>
			<!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
		</div>
	</footer>
</div>
<!--===============================================================================================-->
<script src="~/Assests/js/jquery-3.3.1.min.js"></script>
<script src="~/Assests/styles/bootstrap-4.1.2/popper.js"></script>
<script src="~/Assests/styles/bootstrap-4.1.2/bootstrap.min.js"></script>
<script src="~/Assests/plugins/greensock/TweenMax.min.js"></script>
<script src="~/Assests/plugins/greensock/TimelineMax.min.js"></script>
<script src="~/Assests/plugins/scrollmagic/ScrollMagic.min.js"></script>
<script src="~/Assests/plugins/greensock/animation.gsap.min.js"></script>
<script src="~/Assests/plugins/greensock/ScrollToPlugin.min.js"></script>
<script src="~/Assests/plugins/OwlCarousel2-2.3.4/owl.carousel.js"></script>
<script src="~/Assests/plugins/easing/easing.js"></script>
<script src="~/Assests/plugins/progressbar/progressbar.min.js"></script>
<script src="~/Assests/plugins/parallax-js-master/parallax.min.js"></script>
<script src="~/Assests/plugins/jquery-datepicker/jquery-ui.js"></script>
<script src="~/Assests/js/about.js"></script>
<script src="~/Assests/js/custom.js"></script>
<!--===============================================================================================-->
    <!-- Jquery JS-->
    <script src="~/BokingAssests/vendor/jquery/jquery.min.js"></script>
    <!-- Vendor JS-->
    <script src="~/BokingAssests/vendor/select2/select2.min.js"></script>
    <script src="~/BokingAssests/vendor/datepicker/moment.min.js"></script>
    <script src="~/BokingAssests/vendor/datepicker/daterangepicker.js"></script>
</body>
</html>